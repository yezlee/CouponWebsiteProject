@charset "UTF-8";
@import url("https://fonts.googleapis.com/css?family=Lato:300,400,700,900");
@import url("https://fonts.googleapis.com/css?family=ZCOOL+XiaoWei|Noto+Sans+KR:300,400,500,700,900&display=swap&subset=korean");
@import url(https://cdn.jsdelivr.net/gh/moonspam/NanumSquare@1.0/nanumsquare.css);
@import url(//fonts.googleapis.com/earlyaccess/jejugothic.css);
@import url(//fonts.googleapis.com/earlyaccess/jejumyeongjo.css);
@import url(//fonts.googleapis.com/earlyaccess/kopubbatang.css);
@import url(//fonts.googleapis.com/earlyaccess/nanumbrushscript.css);
@import url(//fonts.googleapis.com/earlyaccess/nanumgothic.css);
@import url(//fonts.googleapis.com/earlyaccess/nanummyeongjo.css);

/* 공통 */
html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp, small, strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, figcaption, figure, footer, header, menu, nav, section, summary, time, mark, audio, video { margin: 0; padding: 0; }

li { list-style: none; }
img, fieldset, iframe { border: 0 none; }
img { vertical-align: top; }
a { outline: none; text-decoration: none; font-size: inherit; color: inherit; }
img, fieldset, iframe { border: 0 none; }
textarea { resize: none; }
h1, h2, h3, h4, h5, h6 { font-size: 100%; font-weight: normal; }
address, strong, caption, cite, code, dfn, em, var { font-style: normal; font-weight: normal; }
input { margin: 0; padding: 0; }
button { border: 0; background-color: transparent; padding: 0; cursor: pointer; outline: none; }
body, input, select, textarea, button { word-break : keep-all; font-family: 'Lato', 'Noto Sans KR', 'Apple SD Gothic Neo', 'Malgun Gothic', arial, 'Nanum Myeongjo', 'Jeju Gothic', 'Jeju Myeongjo', 'KoPub Batang', 'Nanum Brush Script', 'Nanum Gothic', sans-serif!important; font-size: 15px; color: #222; letter-spacing: -0.01em; text-rendering: optimizeLegibility !important; -webkit-font-smoothing: antialiased !important; -moz-osx-font-smoothing: grayscale; }
::selection {background: #ff9900; color: #fff;}
a:hover, a:focus{
	text-decoration: none!important;
	color: inherit!important;
}


#container{
	text-align: center;
}

.push{
	clear : both;
	height : 80px;
}



/*서브공통*/
#container #content_wrap #wrapper{
    max-width: 1100px;
    margin: auto;
    margin-top: 35px;
    text-align : left;
}

#container #content_wrap #wrapper h2{
	text-align: left;
    font-size: 25px;
    font-family: 'NanumSquare';
    font-weight: 600;
    color: #222;
    border-bottom: 1px solid #999;
    padding-bottom: 20px;
}



/* header 공통*/

.header{
	background: #ffb400;
}

.header #navbar{
	width: 1100px;
    margin: auto;
}

.header #navbar .inner{
	display : cell;
	table-layout: fixed;
}
.header #navbar .inner .logo{
	display : table-cell;
	padding-left: 10px;
}

.header #navbar .inner .logo h1{
	width : 280px;
	margin: 0;
}

.header #navbar .inner .logo a{
	background: url(../images/logo.png) no-repeat left center;	
	background-size: 230px;
	display: block;
	text-indent: -9999px;
	height : 115px;
}

	



	a:link{
		color : #222;
		text-decoration: none;
	}
	a:visited{
		color : #222;
	}
	a:hover{
		color : #222;
	}
	a:active{
		color : #222;
	}
	::marker {
	display : none;
	}


.header #navbar .inner .search{
	display : table-cell;
	width : 880px;
    vertical-align: middle;
}

.header #navbar .inner .search input{
	border: none;
    box-sizing: border-box;
    background: transparent;
    border-bottom: 2px solid;	
	width: 80%;
	height: 40px;
}

.header #navbar .inner .search input::placeholder{
	color : #222;	
	font-size : 20px;
	font-family: 'NanumSquare', sans-serif;
	padding-left : 5px;
}

.header #navbar .inner .search img{
	width : 25px;
	padding: 8px 0 0 5px;
}	

.header #navbar .inner .right_menu{
	display : table-cell;
	vertical-align: middle;
	width : 300px;
	text-align: right;
}
.header #navbar .inner .right_menu p{
	display: inline-block;
}
.header #navbar .inner .right_menu p a{
	font-family: 'NanumSquare', sans-serif;
	display: inline-block;
	font-size: 21px;
    font-weight: 900;
    margin-left: 20px;
    color: inherit!important;
}
.header #navbar .inner .right_menu p:hover a,
.header #navbar .inner .right_menu p:focus a,
.header #navbar .inner .right_menu p:active a,
.header #navbar .inner .right_menu p.active a{
	color : #111;
	background: linear-gradient(to bottom, #ffe9b3 0%, #ffe9b3 100%);
    background-position: 0 100%;
    background-repeat: repeat-x;
    background-size: 20px 12px; 
}

	
.header #navbar .inner .right_menu p #cust_on{
	color : #111;
	background: linear-gradient(to bottom, #ffe9b3 0%, #ffe9b3 100%);
    background-position: 0 100%;
    background-repeat: repeat-x;
    background-size: 20px 12px; 
}




.loginbar_wrap{
	background : #333;
	height: 35px;
    box-shadow: 0px 3px 17px 0px rgba(0,0,0,.25);
    z-index: 1;
    position: relative;
}

.loginbar_wrap ul{
	max-width: 1100px;
    text-align: right;
    line-height: 35px;
    margin: auto;
}
	
.loginbar_wrap ul li{
	display: inline-block;
	
}

.loginbar_wrap ul li a{
	color : #fff;
	margin-left : 15px;
	font-size : 15px;
	font-weight: 200;
}

.loginbar_wrap ul li a:hover, a:focus{
	color : #fff!important;
	font-weight: 700;	
}

.loginbar_wrap ul li a.bar:before{
	content: "";
    display: inline-block;
    width: 1px;
    height: 12px;
    background-color: #ccc;
    vertical-align: middle;
    margin: -2px 15px 0 0;
}

.loginbar_wrap ul li #active_hover{
	font-weight: 500;
}


/* body공통 */

body{
	margin : 0;
	height:100%;
	min-width: 1200px;
}

#container{
	position: relative;
	min-height:100%;
}

#container #content_wrap{
	min-height: 100%;
	margin: auto;
/* 	padding-bottom: 80px; */
}





/* main_slide */

.main_slide img{
	width: 100%;
}





/* all_products */


.body .all_products{
	max-width : 1100px;	
	margin: auto;
} 

.body .all_products  h3{
	clear : both;
    padding: 50px 0 20px;
    text-align: left;
    font-weight: 800;
    color: #efaa08;
    font-size: 25px;
    font-family: 'NanumSquare';
}

.body .all_products .filter{
    border-top: solid 2px #333;
}


.body .all_products .filter dl{
	padding: 13px 0;
    border-bottom: solid 1px #dedede;
	height: 30px;    
}


.body .all_products .filter dl dt{
	float: left;
    width: 140px;
    position: relative;
    text-align: left;
    font-size: 16px;
    font-weight: 600;
}

.body .all_products .filter dl dd{
	float: right;
    width: 960px;	
}

.body .all_products .filter dl dd ul{
	height: 50px;
}

.body .all_products .filter dl dd ul li{
	float: left;	
	padding: 0 10px;
	height: 30px;
}

.body .all_products .filter dl dd ul li label{
	font-size : 15px;
} 




.body .all_products .filter_category{
	
}



.body .all_products .filter_category .category{
	
}


.body .all_products .filter_category .category input{
	
}
.body .all_products .filter_category .category label{
	display: inline-block;
}



.body .all_products .products {
	padding-top : 45px;
}


	
.body .all_products .products .arrange_products{
	
}


.body .all_products .products .arrange_products{
	height : 30px;
}


.body .all_products .products .arrange_products li{
	float : left;
	font-size : 13px;
	padding-right: 10px;
    padding-bottom: 10px;  
}


.body .all_products .products .product_ul{
	margin-right: -15px;
}	


.body .all_products .products .product{
	float: left;
    cursor: pointer;
    padding-right: 18px;
    padding-bottom: 30px;
    width: 15.05%;
}

.body .all_products .products .product img{
	
}

.body .all_products .products .product p{
	text-align : left;
}

.body .all_products .products .product .product_wrapper{
	position: relative;
    display: block;
    height : 230px;
}

.body .all_products .products .product .product_wrapper:before{
    content: "";
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    border: solid 1px #eee;
}

.body .all_products .products .product .product_img{
	position: relative;
	max-height: 119px;
    overflow: hidden;
}

.body .all_products .products .product .product_img img{
    display: table-cell;
    width: 85%;
    text-align: center;
    vertical-align: middle;
    border-radius: 0.5em;
    margin: auto;
    margin-top: 10px
}

.body .all_products .products .product .product_img p{
    position: absolute;
    top: 0px;
    right: 0px;
    width: 58px;
    height: 18px;
    line-height: 20px;
    text-align: center;
    font-size: 10px;
    color: #fff;
    font-weight: 800;
    letter-spacing: 0.4px;
    background-color: #333333;
    font-family: 'Lato';
}

.body .all_products .products .product .product_txt{
	padding : 10px 15px;
	
}
.body .all_products .products .product .product_txt .product_before_price{
	font-size : 13px;
	color : #ccc; 
    text-decoration: line-through;
}
.body .all_products .products .product .product_txt .product_price{
	font-size : 27px;
	font-weight : 900;
    letter-spacing: 0.02em;
	color : #efaa08; 
}

.body .all_products .products .product .product_txt .product_name{
	font-size : 15px;
	padding-top : 1px;
}

.body .all_products .products .product .product_txt .product_brand{
	font-size : 11px;
	color : #666;
	text-align: right;
}
