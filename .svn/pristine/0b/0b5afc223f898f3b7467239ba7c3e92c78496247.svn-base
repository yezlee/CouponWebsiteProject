/* footer */

#footer{
	background : #333;
	position: absolute;
	bottom: 0;
	width: 100%;
	height: 80px;
	
}

#footer p{
	font-size: 14px;
    font-weight: 100;
    font-family: 'Lato';
    letter-spacing: 0.5px;
    color: #ddd;
    padding-top: 30px;
}



.fixed_footer{
	position: fixed!important;
}






