html,
body {
  width: 100%;
  -webkit-overflow-scrolling: touch;
}
button {
  outline: none !important;
}
.app {
  height: 100%;
}
#body {
  min-height: 100%;
}
#body .app-body {
  height: 100%;
  padding-bottom: 340px;
}
#footer {
  margin-top: -340px;
}
.lpagination {
  text-align: center;
  width: 100%;
  margin: 0 auto;
}
.lpagination span {
  display: inline-block;
  margin: 0;
  padding: 0;
}
.lpagination span a {
  font-size: 0.9rem;
  letter-spacing: -0.05rem;
  line-height: 1.6;
  letter-spacing: 0!important;
  display: inline-block;
  width: 2.5rem;
  height: 2rem;
  line-height: 2rem;
  text-align: center;
  margin: 0;
  border-left: 1px solid rgba(120, 130, 140, 0.3);
  color: rgba(0, 0, 0, 0.5);
  cursor: pointer;
}
.lpagination span.active a {
  color: #2196f3 !important;
}
.lpagination span:first-child a {
  border-left: 0;
}
/* custom container width */
.container {
  width: 100%;
  margin-right: auto;
  margin-left: auto;
  padding-left: 0rem;
  padding-right: 0rem;
}
@media (max-width: 767px) {
  .container {
    padding-left: 2rem;
    padding-right: 2rem;
  }
}
@media (min-width: 768px) {
  .container {
    max-width: 640px!important;
    padding-left: 0rem;
    padding-right: 0rem;
  }
}
@media (min-width: 992px) {
  .container {
    max-width: 860px!important;
    padding-left: 0rem;
    padding-right: 0rem;
  }
  .container.sm {
    padding-left: 4rem;
    padding-right: 4rem;
  }
}
@media (min-width: 1200px) {
  .container {
    max-width: 960px!important;
    padding-left: 0rem;
    padding-right: 0rem;
  }
  .container.sm {
    padding-left: 6rem;
    padding-right: 6rem;
  }
  .container.lg {
    padding-left: 0;
    padding-right: 0;
    max-width: 1080px!important;
  }
}
.home-banner {
  text-align: center;
}
.home-banner .main {
  font-size: 3rem;
  font-weight: 200;
  letter-spacing: -0.3rem;
  padding: 0.5rem 0 1rem 0;
}
.home-banner .main div.import {
  font-family: georgia;
  letter-spacing: 0;
  font-style: italic;
  margin-left: 0.3rem;
  display: inline-block;
  position: relative;
}
.home-banner .main div.import img {
  position: absolute;
  bottom: -12px;
  left: 50%;
  margin-left: -75%;
  width: 150%;
  opacity: 0.5;
}
.home-banner .sub {
  font-size: 1.4rem;
  letter-spacing: -0.1rem;
  line-height: 1.7;
  opacity: 0.9;
  font-weight: 200;
  margin: 0rem 0;
}
.home-banner img.mac {
  width: 80%;
  margin-bottom: 1rem;
  margin-top: 1rem;
}
.home-banner button {
  font-size: 0.9rem;
  letter-spacing: -0.05rem;
  line-height: 1.6;
  padding: 0.6rem 2.2rem 0.7rem 2.2rem;
  background-color: transparent;
  border: 1px solid rgba(255, 255, 255, 0.5);
  border-radius: 5px;
  -webkit-transition: all 0.5s, -webkit-transform 0.5s;
  transition: all 0.5s, transform 0.5s;
  color: rgba(255, 255, 255, 0.7);
  margin: 2rem 0.5rem;
}
.home-banner button:hover {
  border: 1px solid rgba(255, 255, 255, 0.7);
  -webkit-transition: all 0.5s, -webkit-transform 0.5s;
  transition: all 0.5s, transform 0.5s;
  color: #ffffff;
}
.home-banner button:active {
  position: relative;
  top: 1px;
}
.home-banner button:focus {
  outline: none;
  box-shadow: none;
}
.home-banner .applist {
  text-align: left;
  margin-bottom: 2rem;
  padding-top: 1.5rem;
  padding-left: 3rem;
  padding-right: 3rem;
}
.home-banner .applist .icon {
  float: left;
  border-radius: 10px;
  width: 35px;
  height: 35px;
  margin-right: 11px;
  overflow: hidden;
}
.home-banner .applist .icon img {
  width: 100%;
}
.home-banner .applist .ko {
  font-size: 0.75rem;
  letter-spacing: -0.02rem;
  line-height: 1.6;
  margin: 0;
  padding: 0;
  opacity: 0.6;
  line-height: 1.4;
}
.home-banner .applist .en {
  font-size: 0.75rem;
  letter-spacing: -0.02rem;
  line-height: 1.6;
  letter-spacing: 0!important;
  text-transform: uppercase;
  opacity: 1;
  line-height: 1.5;
  margin-bottom: 1rem;
}
/* footer */
.footer {
  padding: 3rem 0;
  margin: 0;
  width: 100%;
}
.footer * {
  color: #fff!important;
  text-align: center;
}
.footer .fnb li {
  display: inline-block;
  padding: 0.5rem 1rem;
}
.footer .fnb li a {
  font-size: 0.9rem;
  letter-spacing: -0.05rem;
  line-height: 1.6;
}
.footer .snsicon {
  text-align: center;
  padding: 2rem 0 0 0;
}
.footer .snsicon li {
  padding: 0 1rem;
  display: inline-block;
}
.footer .snsicon li i {
  font-size: 1.6rem;
  letter-spacing: -0.09rem;
  line-height: 1.6;
  opacity: 0.3;
}
.footer .info {
  text-align: center;
  padding-top: 2rem;
  padding-bottom: 2rem;
}
.footer .info li {
  font-size: 0.85rem;
  letter-spacing: -0.04rem;
  line-height: 1.6;
  display: inline-block;
  opacity: 0.7;
}
.footer .info li span {
  opacity: 0.5;
  margin: 0 0.5rem;
}
@media (max-width: 567px) {
  .footer {
    padding: 3rem 2rem;
  }
}
